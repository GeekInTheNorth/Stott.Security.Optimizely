{
    "id": 2,
    "name": "Strict Policy",
    "scopeType": "Site",
    "scopeId": 1,
    "scopeName": "Site Scope",
    "scopeBehavior": "Content",
    "scopePaths": [
        "/"
    ],
    "scopeExclusions": [
        "/ui/",
        "/util/"
    ],
    "sources": [
        { "url": "self", "directives": ["script-src", "style-src"] },
        { "url": "https://trusted.example.com", "directives": ["script-src", "style-src"] },
        { "url": "https://secure-cdn.example.com", "directives": ["script-src", "style-src"] },
        { "url": "https://fonts.googleapis.com", "directives": ["font-src"] },
        { "url": "https://fonts.gstatic.com", "directives": ["font-src"] }
    ],
    "isEnabled": true,
    "isReportOnly": false,
    "useInternalReporting": true,
    "useExternalReporting": false,
    "externalReportToUrl": "https://csp-report.example.com/report",
    "isUpgradeInsecureRequestsEnabled": true,
    "isNonceEnabled": false,
    "isStrictDynamicEnabled": false
}